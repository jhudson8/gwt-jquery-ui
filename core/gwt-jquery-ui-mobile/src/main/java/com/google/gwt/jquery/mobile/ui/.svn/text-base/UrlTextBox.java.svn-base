package com.google.gwt.jquery.mobile.ui;

import com.google.gwt.dom.client.Element;


public class UrlTextBox extends TextBox {

	public UrlTextBox() {
		super();
		getElement().setAttribute("type", "url");
	}

	protected UrlTextBox(Element element) {
		super(element);
		element.setAttribute("type", "url");
	}	
}